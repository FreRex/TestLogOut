  "name": "chat-operativa",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server.js",
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.1",
    "adm-zip": "^0.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "dateformat": "^1.0.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "os": "^0.1.1",
    "pg": "^8.5.1",
    "util": "^0.12.3",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/file-saver": "^2.0.2",
    "@types/node": "^14.14.31",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "ts-node-dev": "^1.1.3"
  },
  "scripts": {
    "prepare": "husky install",
    "p:quick": "pretty-quick",
    "p:check": "prettier --config ./frontend/.prettierrc --check \"frontend/src/{app,environments}/**/*{.ts,.js,.json,.html,.css,.scss,.md}\"",
    "p:format": "prettier --config ./frontend/.prettierrc --write \"frontend/src/{app,environments}/**/*{.ts,.js,.json,.html,.css,.scss,.md}\"",
    "dback": "tsc --build && NODE_ENV=development node ./build/server.js",
    "pback": "tsc --build && NODE_ENV=production node -r dotenv/config ./build/server.js",
    "dev": "tsc --build && cd frontend && ionic build && cd .. && NODE_ENV=development node ./build/server.js",
    "prod": "tsc --build && cd frontend && ionic build --prod && cd .. && NODE_ENV=production node -r dotenv/config ./build/server.js"
  },
